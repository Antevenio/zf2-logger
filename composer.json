{
    "name" : "eddiejaoude/zf2-logger",
    "description" : "Zend Framework 2 Logger - request & response log",
    "type" : "library",
    "authors" : [{
        "name" : "Eddie Jaoude",
        "email" : "eddie@jaoudestudios.com"
    }
    ],
    "license": "MIT",
    "keywords" : [
        "zend",
        "zend framework 2",
        "zf2",
        "logger",
        "log"
    ],
    "require" : {
        "php" : ">=5.3.3",
        "zendframework/zend-log" : "2.* || 3.*",
        "zendframework/zend-di" : "2.* || 3.*",
        "zendframework/zend-eventmanager" : "2.* || 3.*",
        "zendframework/zend-mvc" : "2.* || 3.*"
    },
    "require-dev" : {
        "phpunit/phpunit": "4.*",
        "mockery/mockery" : "0.9.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload" : {
        "psr-4" : {
            "EddieJaoude\\Zf2Logger\\" : "src/Zf2Logger"
        },
        "classmap": [
            "Module.php"
        ]
    }
}
